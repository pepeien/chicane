#victory {
    position: absolute;
    height: 30vh;
    width: 100%;
    margin-top: 10vh;
    background-color: #333333;
}

#victoryText {
    alignment: center;
}

#frameTelemetry {
    position: absolute;
    width: 175px;
    height: 25px;
    margin-left: auto;
    background-color: #333333;
}

#framePerSecond {
    margin-left: 1vw;
    alignment: start center;
    color: #24f51d;
}

#frameTimeText {
    alignment: end center;
    color: #24f51d;
}

.crosshair {
    position: absolute;
    background-color: #FFFFFF;
}

#crosshairTop {
    height: 10px;
    width: 2px;
    margin: 50% 2px 28px 50%;
}

#crosshairBottom {
    height: 10px;
    width: 2px;
    margin: 50% 2px 8px 50%;
}

#crosshairLeft {
    height: 2px;
    width: 10px;
    margin: 50% 15px 15px 50%;
}

#crosshairRight {
    height: 2px;
    width: 10px;
    margin: 50% -5px 15px 50%;
}